{
  "name": "hp-components-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-readme": "copy .\\README.md .\\dist\\hp-components",
    "build_lib": "ng build --prod hp-components",
    "npm_pack": "cd dist/hp-components && npm pack",
    "package": "npm run build_lib && npm run copy-readme && npm run npm_pack",
    "build_git_page": "ng build --prod --base-href \"https://hopetonpalmer.github.io/hp_components/\"",
    "publish_git_page": "ngh --dir=dist/hp-components-app",
    "update_git_page": "npm run build_git_page && npm run publish_git_page",
    "build_lib2": "ng build --prod hp-widgets",
    "npm_pack2": "cd dist/hp-widgets && npm pack",
    "package2": "npm run build_lib2 && npm run npm_pack2",
    "build_sched": "ng build --prod scheduler",
    "npm_pack_sched": "cd dist/hp-components/scheduler && npm pack",
    "package-sched": "npm run build_sched && npm run npm_pack_sched",
    "build_core": "ng build --prod core",
    "npm_pack_core": "cd dist/hp-components/core && npm pack",
    "package-core": "npm run build_core && npm run npm_pack_core",
    "build_fm": "ng build --prod filemanager",
    "npm_pack_fm": "cd dist/hp-components/filemanager && npm pack",
    "package-fm": "npm run build_fm && npm run npm_pack_fm",
    "build_common": "ng build --prod common",
    "npm_pack_common": "cd dist/hp-components/common && npm pack",
    "package-common": "npm run build_common && npm run npm_pack_common"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ctrl/tinycolor": "^2.2.1",
    "@hp-components/common": "file:dist/hp-components/common/hp-components-common-0.0.1.tgz",
    "@hp-components/core": "file:dist/hp-components/core/hp-components-core-0.0.1.tgz",
    "@hp-components/scheduler": "file:dist/hp-components/scheduler/hp-components-scheduler-0.0.1.tgz",
    "core-js": "^2.5.4",
    "date-fns": "^1.29.0",
    "hp-components": "file:dist/hp-components/hp-components-0.4.7.tgz",
    "lodash-uuid": "0.0.3",
    "resize-observer": "^1.0.0-alpha.1",
    "rxjs": "~6.2.0",
    "shortid": "^2.2.13",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~6.2.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.116",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.1.0",
    "protractor": "~5.4.0",
    "reflect-metadata": "^0.1.12",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
