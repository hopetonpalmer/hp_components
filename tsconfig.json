{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "hp-components/*": [
        "dist/hp-components/*"
      ],
      "hp-components-src": [
        "projects/hp-components/src/public_api"
      ],
      "hp-widgets": [
        "dist/hp-widgets"
      ],
      "hp-widgets/*": [
        "dist/hp-widgets/*"
      ],
      "@hp-components/core": [
        "dist/hp-components/core"
      ],
      "@hp-components/core/*": [
        "dist/hp-components/core/*"
      ],
      "@hp-components/core-src": [
        "projects/hp-components/core/src/public_api"
      ],
      "@hp-components/scheduler": [
        "dist/hp-components/scheduler"
      ],
      "@hp-components/scheduler/*": [
        "dist/hp-components/scheduler/*"
      ],
      "@hp-components/scheduler-src": [
        "projects/hp-components/scheduler/src/public_api"
      ],
      "@hp-components/filemanager": [
        "dist/hp-components/filemanager"
      ],
      "@hp-components/filemanager/*": [
        "dist/hp-components/filemanager/*"
      ],
      "@hp-components/designer": [
        "dist/hp-components/designer"
      ],
      "@hp-components/designer/*": [
        "dist/hp-components/designer/*"
      ],
      "@hp-components/ui": [
        "dist/hp-components/ui"
      ],
      "@hp-components/ui/*": [
        "dist/hp-components/ui/*"
      ],
      "@hp-components/common": [
        "dist/hp-components/common"
      ],
      "@hp-components/common-src": [
        "projects/hp-components/common/src/public_api"
      ],
      "@hp-components/common/*": [
        "dist/hp-components/common/*"
      ],
      "@hp-components/widgets": [
        "dist/hp-components/widgets"
      ],
      "@hp-components/widgets/*": [
        "dist/hp-components/widgets/*"
      ],
      "@hp-components/file-manager": [
        "dist/hp-components/file-manager"
      ],
      "@hp-components/file-manager/*": [
        "dist/hp-components/file-manager/*"
      ]
    }
  }
}
